    <sql id="selectAwDetailmaterialsubscriptionVo">
        select materialSubscription, subscribeID, productionTasksID, materialID, name, materialPrice,typeName, specificationsType, specificationModel, materialDensity, processingTechnologyID, subscriptionQuantity, requiredDate, sampleURL, note from aw_Detailmaterialsubscription
    </sql>

    <select id="selectAwDetailmaterialsubscriptionList" parameterType="AwDetailmaterialsubscription" resultMap="AwDetailmaterialsubscriptionResult">
        <include refid="selectAwDetailmaterialsubscriptionVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="typeName != null  and typeName != ''"> and typeName like concat('%', #{typeName}, '%')</if>
            <if test="specificationModel != null  and specificationModel != ''"> and specificationModel = #{specificationModel}</if>
            <if test="subscribeID != null  and materialID != ''"> and subscribeID = #{subscribeID}</if>
            <if test="processingTechnologyID != null  and processingTechnologyID != ''"> and processingTechnologyID = #{processingTechnologyID}</if>
            <if test="requiredDate != null "> and requiredDate = #{requiredDate}</if>
        </where>
    </select>
    
    <select id="selectAwDetailmaterialsubscriptionByMaterialSubscription" parameterType="String" resultMap="AwDetailmaterialsubscriptionResult">
        <include refid="selectAwDetailmaterialsubscriptionVo"/>
        where materialSubscription = #{materialSubscription}
    </select>
        
    <insert id="insertAwDetailmaterialsubscription" parameterType="AwDetailmaterialsubscription">
        insert into aw_Detailmaterialsubscription
        <trim prefix="(" suffix=")" suffixOverrides=",">
