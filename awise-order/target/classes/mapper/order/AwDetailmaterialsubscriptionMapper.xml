        </trim>
        where materialSubscription = #{materialSubscription}
    </update>

    <delete id="deleteAwDetailmaterialsubscriptionByMaterialSubscription" parameterType="String">
        delete from aw_Detailmaterialsubscription where materialSubscription = #{materialSubscription}
    </delete>

    <delete id="deleteAwDetailmaterialsubscriptionByMaterialSubscriptions" parameterType="String">
        delete from aw_Detailmaterialsubscription where materialSubscription in
