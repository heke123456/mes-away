            <if test="specificationsType != null">specificationsType,</if>
            <if test="specificationModel != null">specificationModel,</if>
            <if test="materialDensity != null and materialDensity != ''">materialDensity,</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">processingTechnologyID,</if>
            <if test="subscriptionQuantity != null">subscriptionQuantity,</if>
            <if test="requiredDate != null">requiredDate,</if>
            <if test="sampleURL != null">sampleURL,</if>
            <if test="note != null">note,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="materialSubscription != null and materialSubscription != ''">#{materialSubscription},</if>
            <if test="subscribeID != null and subscribeID != ''">#{subscribeID},</if>
            <if test="productionTasksID != null">#{productionTasksID},</if>
            <if test="materialID != null and materialID != ''">#{materialID},</if>
            <if test="name != null and name != ''">#{name},</if>
