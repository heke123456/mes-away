            <if test="processingTechnologyID != null and processingTechnologyID != ''">#{processingTechnologyID},</if>
            <if test="subscriptionQuantity != null">#{subscriptionQuantity},</if>
            <if test="requiredDate != null">#{requiredDate},</if>
            <if test="sampleURL != null">#{sampleURL},</if>
            <if test="note != null">#{note},</if>
         </trim>
