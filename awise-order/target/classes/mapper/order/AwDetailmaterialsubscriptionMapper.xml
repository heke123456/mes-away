            <if test="subscriptionQuantity != null">subscriptionQuantity = #{subscriptionQuantity},</if>
            <if test="requiredDate != null">requiredDate = #{requiredDate},</if>
            <if test="sampleURL != null">sampleURL = #{sampleURL},</if>
