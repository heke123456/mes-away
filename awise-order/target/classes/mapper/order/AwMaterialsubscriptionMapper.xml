<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.awise.order.mapper.AwMaterialsubscriptionMapper">
    
    <resultMap type="AwMaterialsubscription" id="AwMaterialsubscriptionResult">
        <result property="materialSubscription"    column="materialSubscription"    />
        <result property="subscribeID"    column="subscribeID"    />
        <result property="productionTasksID"    column="productionTasksID"    />
        <result property="materialID"    column="materialID"    />
        <result property="processingTechnologyID"    column="processingTechnologyID"    />
        <result property="subscriptionQuantity"    column="subscriptionQuantity"    />
        <result property="requiredDate"    column="requiredDate"    />
        <result property="sampleURL"    column="sampleURL"    />
        <result property="note"    column="note"    />
    </resultMap>

    <sql id="selectAwMaterialsubscriptionVo">
        select materialSubscription, subscribeID, productionTasksID, materialID, processingTechnologyID, subscriptionQuantity, requiredDate, sampleURL, note from aw_MaterialSubscription
    </sql>

    <select id="selectAwMaterialsubscriptionList" parameterType="AwMaterialsubscription" resultMap="AwMaterialsubscriptionResult">
        <include refid="selectAwMaterialsubscriptionVo"/>
        <where>  
            <if test="subscribeID != null  and subscribeID != ''"> and subscribeID = #{subscribeID}</if>
            <if test="productionTasksID != null "> and productionTasksID = #{productionTasksID}</if>
            <if test="materialID != null  and materialID != ''"> and materialID = #{materialID}</if>
            <if test="processingTechnologyID != null  and processingTechnologyID != ''"> and processingTechnologyID = #{processingTechnologyID}</if>
            <if test="subscriptionQuantity != null "> and subscriptionQuantity = #{subscriptionQuantity}</if>
            <if test="requiredDate != null "> and requiredDate = #{requiredDate}</if>
            <if test="sampleURL != null  and sampleURL != ''"> and sampleURL = #{sampleURL}</if>
            <if test="note != null  and note != ''"> and note = #{note}</if>
        </where>
    </select>
    
    <select id="selectAwMaterialsubscriptionByMaterialSubscription" parameterType="String" resultMap="AwMaterialsubscriptionResult">
        <include refid="selectAwMaterialsubscriptionVo"/>
        where materialSubscription = #{materialSubscription}
    </select>
        
    <insert id="insertAwMaterialsubscription" parameterType="AwMaterialsubscription">
        insert into aw_MaterialSubscription
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="materialSubscription != null">materialSubscription,</if>
            <if test="subscribeID != null and subscribeID != ''">subscribeID,</if>
            <if test="productionTasksID != null">productionTasksID,</if>
            <if test="materialID != null and materialID != ''">materialID,</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">processingTechnologyID,</if>
            <if test="subscriptionQuantity != null">subscriptionQuantity,</if>
            <if test="requiredDate != null">requiredDate,</if>
            <if test="sampleURL != null">sampleURL,</if>
            <if test="note != null">note,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="materialSubscription != null">#{materialSubscription},</if>
            <if test="subscribeID != null and subscribeID != ''">#{subscribeID},</if>
            <if test="productionTasksID != null">#{productionTasksID},</if>
            <if test="materialID != null and materialID != ''">#{materialID},</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">#{processingTechnologyID},</if>
            <if test="subscriptionQuantity != null">#{subscriptionQuantity},</if>
            <if test="requiredDate != null">#{requiredDate},</if>
            <if test="sampleURL != null">#{sampleURL},</if>
            <if test="note != null">#{note},</if>
         </trim>
    </insert>

    <update id="updateAwMaterialsubscription" parameterType="AwMaterialsubscription">
        update aw_MaterialSubscription
        <trim prefix="SET" suffixOverrides=",">
            <if test="subscribeID != null and subscribeID != ''">subscribeID = #{subscribeID},</if>
            <if test="productionTasksID != null">productionTasksID = #{productionTasksID},</if>
            <if test="materialID != null and materialID != ''">materialID = #{materialID},</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">processingTechnologyID = #{processingTechnologyID},</if>
            <if test="subscriptionQuantity != null">subscriptionQuantity = #{subscriptionQuantity},</if>
            <if test="requiredDate != null">requiredDate = #{requiredDate},</if>
            <if test="sampleURL != null">sampleURL = #{sampleURL},</if>
            <if test="note != null">note = #{note},</if>
        </trim>
        where materialSubscription = #{materialSubscription}
    </update>

    <delete id="deleteAwMaterialsubscriptionByMaterialSubscription" parameterType="String">
        delete from aw_MaterialSubscription where materialSubscription = #{materialSubscription}
    </delete>

    <delete id="deleteAwMaterialsubscriptionByMaterialSubscriptions" parameterType="String">
        delete from aw_MaterialSubscription where materialSubscription in
        <foreach item="materialSubscription" collection="array" open="(" separator="," close=")">
            #{materialSubscription}
        </foreach>
    </delete>
</mapper>