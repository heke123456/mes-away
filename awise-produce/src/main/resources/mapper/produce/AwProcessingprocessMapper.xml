<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.awise.produce.mapper.AwProcessingprocessMapper">
    
    <resultMap type="AwProcessingprocess" id="AwProcessingprocessResult">
        <result property="id"    column="id"    />
        <result property="processingTechnologyID"    column="processingTechnologyID"    />
        <result property="number"    column="number"    />
        <result property="name"    column="name"    />
        <result property="content"    column="content"    />
        <result property="diagramURL"    column="diagramURL"    />
        <result property="usedTooling"    column="usedTooling"    />
        <result property="preparationHours"    column="preparationHours"    />
        <result property="taktTime"    column="taktTime"    />
        <result property="laborCost"    column="laborCost"    />
        <result property="outsourcing"    column="outsourcing"    />
        <result property="status"   column="status"   />
        <result property="quantity"   column="quantity"   />
    </resultMap>

    <sql id="selectAwProcessingprocessVo">
        SELECT
        id,
        processingTechnologyID,
        number,
        NAME,
        content,
        diagramURL,
        usedTooling,
        preparationHours,
        taktTime,
        laborCost,
        outsourcing,
        status,
        quantity
        FROM
        aw_processingprocess
    </sql>

    <select id="selectAwProcessingprocessList" parameterType="AwProcessingprocess" resultMap="AwProcessingprocessResult">
        <include refid="selectAwProcessingprocessVo"/>
        <where>  
            <if test="processingTechnologyID != null  and processingTechnologyID != ''"> and processingTechnologyID = #{processingTechnologyID}</if>
            <if test="number != null "> and number = #{number}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="diagramURL != null  and diagramURL != ''"> and diagramURL = #{diagramURL}</if>
            <if test="usedTooling != null  and usedTooling != ''"> and usedTooling = #{usedTooling}</if>
            <if test="preparationHours != null  and preparationHours != ''"> and preparationHours = #{preparationHours}</if>
            <if test="taktTime != null  and taktTime != ''"> and taktTime = #{taktTime}</if>
            <if test="laborCost != null  and laborCost != ''"> and laborCost = #{laborCost}</if>
            <if test="outsourcing != null  and outsourcing != ''"> and outsourcing = #{outsourcing}</if>
            <if test="status != null and status != ''">and status = #{status}</if>
            <if test="quantity != null and quantity != ''">and quantity = #{quantity}</if>
        </where>
    </select>
    
    <select id="selectAwProcessingprocessById" parameterType="Long" resultMap="AwProcessingprocessResult">
        <include refid="selectAwProcessingprocessVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertAwProcessingprocess" parameterType="AwProcessingprocess">
        insert into aw_processingprocess
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">processingTechnologyID,</if>
            <if test="number != null">number,</if>
            <if test="name != null and name != ''">name,</if>
            <if test="content != null and content != ''">content,</if>
            <if test="diagramURL != null">diagramURL,</if>
            <if test="usedTooling != null and usedTooling != ''">usedTooling,</if>
            <if test="preparationHours != null and preparationHours != ''">preparationHours,</if>
            <if test="taktTime != null and taktTime != ''">taktTime,</if>
            <if test="laborCost != null and laborCost != ''">laborCost,</if>
            <if test="outsourcing != null and outsourcing != ''">outsourcing,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="quantity != null ">quantity,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="processingTechnologyID != null and processingTechnologyID != ''">#{processingTechnologyID},</if>
            <if test="number != null">#{number},</if>
            <if test="name != null and name != ''">#{name},</if>
            <if test="content != null and content != ''">#{content},</if>
            <if test="diagramURL != null">#{diagramURL},</if>
            <if test="usedTooling != null and usedTooling != ''">#{usedTooling},</if>
            <if test="preparationHours != null and preparationHours != ''">#{preparationHours},</if>
            <if test="taktTime != null and taktTime != ''">#{taktTime},</if>
            <if test="laborCost != null and laborCost != ''">#{laborCost},</if>
            <if test="outsourcing != null and outsourcing != ''">#{outsourcing},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="quantity != null ">#{quantity},</if>
         </trim>
    </insert>

    <update id="updateAwProcessingprocess" parameterType="AwProcessingprocess">
        update aw_processingprocess
        <trim prefix="SET" suffixOverrides=",">
            <if test="processingTechnologyID != null and processingTechnologyID != ''">processingTechnologyID = #{processingTechnologyID},</if>
            <if test="number != null">number = #{number},</if>
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="content != null and content != ''">content = #{content},</if>
            <if test="diagramURL != null">diagramURL = #{diagramURL},</if>
            <if test="usedTooling != null and usedTooling != ''">usedTooling = #{usedTooling},</if>
            <if test="preparationHours != null and preparationHours != ''">preparationHours = #{preparationHours},</if>
            <if test="taktTime != null and taktTime != ''">taktTime = #{taktTime},</if>
            <if test="laborCost != null and laborCost != ''">laborCost = #{laborCost},</if>
            <if test="outsourcing != null and outsourcing != ''">outsourcing = #{outsourcing},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="quantity != null">quantity = #{quantity},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteAwProcessingprocessById" parameterType="Long">
        delete from aw_processingprocess where id = #{id}
    </delete>

    <delete id="deleteAwProcessingprocessByIds" parameterType="String">
        delete from aw_processingprocess where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>