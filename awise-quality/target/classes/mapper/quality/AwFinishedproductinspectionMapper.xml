    <delete id="deleteAwFinishedproductinspectionById" parameterType="String">
        delete from aw_FinishedProductInspection where id = #{id}
    </delete>

    <delete id="deleteAwFinishedproductinspectionByIds" parameterType="String">
        delete from aw_FinishedProductInspection where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
