    <sql id="selectAwFinishedproductinspectionVo">
        select id, nameOfQualityInspection, productionTasksID, productID, detectionQuantity, qualifiedQuantity, unqualifiedQuantity, testDate, testResult, testingPersonnel, notes from aw_FinishedProductInspection
    </sql>

    <select id="selectAwFinishedproductinspectionList" parameterType="AwFinishedproductinspection" resultMap="AwFinishedproductinspectionResult">
        <include refid="selectAwFinishedproductinspectionVo"/>
        <where>  
            <if test="nameOfQualityInspection != null  and nameOfQualityInspection != ''"> and nameOfQualityInspection = #{nameOfQualityInspection}</if>
            <if test="testResult != null  and testResult != ''"> and testResult = #{testResult}</if>
            <if test="testDate != null "> and testDate = #{testDate}</if>
            <if test="testingPersonnel != null  and testingPersonnel != ''"> and testingPersonnel = #{testingPersonnel}</if>
        </where>
    </select>
    
    <select id="selectAwFinishedproductinspectionById" parameterType="String" resultMap="AwFinishedproductinspectionResult">
        <include refid="selectAwFinishedproductinspectionVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertAwFinishedproductinspection" parameterType="AwFinishedproductinspection">
        insert into aw_FinishedProductInspection
        <trim prefix="(" suffix=")" suffixOverrides=",">
